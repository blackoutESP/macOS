{
    "workbench.colorTheme": "Community Material Theme Darker",
    "workbench.colorCustomizations": {
        "editorLineNumber.foreground": "#2aa198"
    },
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "eq-material-theme-icons-darker",
    "editor.formatOnPaste": false,
    "editor.formatOnSave": false,
    "editor.formatOnType": true,
    "editor.suggestSelection": "first",
    "editor.fontFamily": "Fira Code",
    "terminal.integrated.fontFamily": "Fira Code",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "touchBar.dayTheme": "Community Material Theme Lighter",
    "touchBar.nightTheme": "Community Material Theme Darker",
    "touchBar.goToReferences": true,
    "touchBar.organizeImports": true,
    "touchBar.showExplorer": false,
    "touchBar.search": false,
    "touchBar.extensions": false,
    "touchBar.themeToggle": true,
    "touchBar.terminal": true,
    "touchBar.toggleZenMode": false,
    "window.zoomLevel": 0,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "redhat.telemetry.enabled": true,
    "editor.fontFamily": "'Fira Code'",
    "editor.fontLigatures": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.formatOnSave": false
    },
    "prettier.jsxSingleQuote": true,
    "prettier.tabWidth": 4,
    "editor.formatOnSave": false
}
